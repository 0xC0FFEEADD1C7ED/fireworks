// shortcut: CTRL + ALT + H
/**DOC**
Sets the height of the canvas.
**DOC**/
try {
  fw.runScript(fw.appJsCommandsDir + "/bs.js");
} catch(e){
  alert("This command requires the bs.js library\rGet it at http://bomberstudios.com/fireworks/");
};

var default_height = dom().height;

var h = prompt("Canvas height in pixels",default_height);

dom().setDocumentCanvasSize({left:0, top:0, right:dom().width, bottom:eval(h)}, true);