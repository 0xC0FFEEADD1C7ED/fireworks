/**DOC**
Centers the selected object(s) on the canvas, both horizontally and vertically.
**DOC**/
try {
  fw.runScript(fw.appJsCommandsDir + "/bs.js");
} catch(e){
  alert("This command requires the bs.js library\rGet it at http://github.com/bomberstudios/fireworks/");
};

fw.selection.each(function(e){
  e.top = Math.round(fw.getDocumentDOM().height/2 - e.height/2);
  e.left = Math.round(fw.getDocumentDOM().width/2 - e.width/2);
},false);